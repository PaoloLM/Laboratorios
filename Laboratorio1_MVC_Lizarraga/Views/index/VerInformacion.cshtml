
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>VerInformacion</title>
</head>
<body>
    <h1>Informacion Personal</h1>
    <br />
    <div>
        <b>Nombre: </b>@ViewBag.Nombre <br />
        <b>Apellido: </b>@ViewBag.Apellido <br />
        <b>Edad: </b>@ViewBag.Edad <br />

        <b>Talla: </b>@ViewData["Talla"] <br />
        <b>Sexo: </b>@ViewData["Sexo"] <br />
        <b>Fecha y Hora: </b>@ViewData["FechaHora"] <br />

        @Html.TextBox("apellido", ViewData["Apellido"]) <br />
        @Html.TextBox("talla", ViewData["Talla"]) <br />
    </div>
</body>
</html>
